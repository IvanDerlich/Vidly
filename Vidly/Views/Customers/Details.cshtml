@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
@*<h2>ID: @Model.Id</h2>*@
<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if (!Model.BirthDate.IsEmpty())
    {
        <li>Birth Date: @Model.BirthDate</li>
    }
</ul>
<hr>


